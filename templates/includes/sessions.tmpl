{{define "content"}}
<div class="row">
	<div class="col">
		<h1>Experiments</h1>
		<table class="table">
			<thead>
				<tr>
					<th>id</th>
					<th>type</th>
					<th>participants</th>
					<th>created at</th>
					<th>oTree admin</th>
				</tr>
			</thead>
			<tbody>
				{{range .Sessions}}
				<tr>
					<td>{{ .Id }}</td>
					<td>{{ .ConfigName }}</td>
					<td>{{ .NumParticipants }}</td>
					<td>{{ .FormatCreatedAt }}</td>
					<td><a href="{{ .AdminUrl }}" target="_blank">{{ .Code }}</a></td>
				</tr>
				{{end}}
			</tbody>
		</table>
	</div>
</div>
{{end}}